          <stringProp name="script">import groovy.json.JsonSlurper;
import groovy.json.JsonOutput;

def jsonSlurper = new JsonSlurper();

String newline = System.getProperty(&quot;line.separator&quot;);
def url = vars.get(&quot;v_urlCsv&quot;);
String header = &quot;idTest,v_username,v_pass,v_cat,v_id,v_title&quot;+newline;//Encabezados

String testID = vars.get(&quot;idTest&quot;);//*********
String username = vars.get(&quot;v_username&quot;);//*********
String pass = vars.get(&quot;v_pass&quot;);//*********



String fileLocation = vars.get(&quot;JMETER_SCRIPTS_DIRECTORY&quot;)+&quot;logs&quot;+vars.get(&quot;fileSystemSeparator&quot;)+&quot;data_&quot;+vars.get(&quot;outputFile&quot;);

File tmpDir = new File(fileLocation);
empty = tmpDir.length() == 0;
FileWriter data = new FileWriter(fileLocation, true);

BufferedWriter out = new BufferedWriter(data);
if (empty){out.write(header);}


///**********************************
/*File tmpDir = new File(url);//Crea el archivo
empty = tmpDir.length() == 0;//Validando si el archivo esta vacio
