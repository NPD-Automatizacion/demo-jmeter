if (empty){out.write(header);}//Valida si la cabecera ya fue creada no la vuelve a escribir*/

//************************************

if (testID == &quot;1&quot;){
	def jsonP = jsonSlurper.parseText(vars.get(&quot;v_phone&quot;));
	def jsonItemsP = jsonP.Items;
	for (i = 0; i &lt; jsonItemsP.size(); i++){
		
		out.write(testID+&quot;,&quot;+username+&quot;,&quot;+pass+&quot;,&quot;+jsonItemsP[i].cat+&quot;,&quot;+jsonItemsP[i].id+&quot;,&quot;+jsonItemsP[i].title+&quot;&quot;+newline);
	}
}

if (testID == &quot;2&quot;){
	def jsonN = jsonSlurper.parseText(vars.get(&quot;v_notebook&quot;));
	def jsonItemsN = jsonN.Items;
	for (j = 0; j &lt; jsonItemsN.size(); j++){
		
